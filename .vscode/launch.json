    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (App)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build frontend",
            "program": "${workspacefolder}/bin/Debug/AutoDarkModeApp.dll",
            "args": [],
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (Svc)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}\\bin\\Debug\\AutoDarkModeSvc.dll",
            "cwd": "${workspaceFolder}\\bin\\Debug\\",
            "args": [],
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
